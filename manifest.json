{
  "manifest_version": 3,
  "name": "Email AutoFill Extension",
  "version": "1.0",
  "description": "Searches for input with name='email' and fills it with 'myEmail'.",
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "16": "icons/iconOne.png",
    "48": "icons/iconOne.png",
    "128": "icons/iconOne.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
